# Backend configuration
PORT=backend-port

# Google OAuth credentials (get from https://console.cloud.google.com/)
GOOGLE_CLIENT_ID=your-client-id-here.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret-here

# JWT secret
JWT_SECRET=your-jwt-secret-here

# Application URLs
FRONTEND_URL=http://host:port
CALLBACK_URL=http://host:port/api/...

# External APIs
STABLE_DIFFUSION_API_URL=http://stable-diffusion.your.private.endpoint
POKEAPIKEY=your-api-key-here
